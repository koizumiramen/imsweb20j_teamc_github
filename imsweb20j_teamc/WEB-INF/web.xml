<web-app>

	<!-- LoginServlet -->

	<servlet>
		<servlet-name>loginMapping</servlet-name>
		<servlet-class>servlet.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>loginMapping</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>


	<!-- ListServlet -->

	<servlet>
		<servlet-name>listMapping</servlet-name>
		<servlet-class>servlet.ListServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>listMapping</servlet-name>
		<url-pattern>/list</url-pattern>
	</servlet-mapping>



	<!-- InquiryFormServlet -->

	<servlet>
		<servlet-name>inquiryFormMapping</servlet-name>
		<servlet-class>servlet.InquiryFormServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>inquiryFormMapping</servlet-name>
		<url-pattern>/inquiryForm</url-pattern>
	</servlet-mapping>



	<!-- InquiryInsertServlet -->

	<servlet>
		<servlet-name>inquiryInsertMapping</servlet-name>
		<servlet-class>servlet.InquiryInsertServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>inquiryInsertMapping</servlet-name>
		<url-pattern>/inquiryInsert</url-pattern>
	</servlet-mapping>



	<!-- InquiryReplyFormServlet -->

	<servlet>
		<servlet-name>inquiryReplyFormMapping</servlet-name>
		<servlet-class>servlet.InquiryReplyFormServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>inquiryReplyFormMapping</servlet-name>
		<url-pattern>/inquiryReplyForm</url-pattern>
	</servlet-mapping>



	<!-- ReplyConfirmServlet -->

	<servlet>
		<servlet-name>replyConfirmMapping</servlet-name>
		<servlet-class>servlet.ReplyConfirmServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>replyConfirmMapping</servlet-name>
		<url-pattern>/replyConfirm</url-pattern>
	</servlet-mapping>



	<!-- SendMailServlet -->

	<servlet>
		<servlet-name>sendMailMapping</servlet-name>
		<servlet-class>servlet.SendMailServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>sendMailMapping</servlet-name>
		<url-pattern>/sendMail</url-pattern>
	</servlet-mapping>

</web-app>